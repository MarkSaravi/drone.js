{
    "name": "channel-9-web-api-coding-challenge",
    "version": "1.0.0",
    "description": "channel 9 Web Api Coding Challenge",
    "author": "Mark Saravi",
    "license": "ISC",
    "scripts": {
        "clean": "rm -rf dist && mkdir dist",
        "build": "npm run clean && babel -d ./dist ./src",
        "prodserver": "node ./dist/server.js",
        "start": "export NODE_ENV=production && npm run build && npm run prodserver",
        "devserver": "npm-run-all --parallel test rundev lint",
        "rundev": "export NODE_ENV=development && babel-node src/server.js",
        "lint": "node_modules/.bin/esw src",
        "buildtests": "npm run clean && babel -d ./dist ./test",
        "runtests": "mocha --reporter progress test/testSetup.js \"test/**/*.test.js\"",
        "test": "npm run buildtests && npm run runtests"
    },
    "dependencies": {
        "babel-cli": "6.8.0",
        "babel-core": "6.8.0",
        "babel-polyfill": "6.8.0",
        "babel-preset-es2015": "6.6.0",
        "babel-register": "6.8.0"
    },
    "devDependencies": {
        "eslint": "2.9.0",
        "eslint-plugin-import": "^2.12.0",
        "eslint-watch": "2.1.11",
        "expect": "1.19.0",
        "mocha": "2.4.5",
        "mocha-each": "^1.1.0",
        "mock-express-request": "^0.2.2",
        "mock-express-response": "^0.2.2",
        "npm-run-all": "1.8.0"
    },
    "repository": {
        "type": "git",
        "url": ""
    }
}